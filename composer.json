{
  "name": "contributte/componette-site",
  "description": "Addons, plugins and components for Nette Framework",
  "keywords": [
    "nette",
    "components",
    "addons",
    "extensions"
  ],
  "homepage": "https://componette.org",
  "type": "project",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "Componette Contributors",
      "homepage": "https://github.com/contributte/contributte-site/graphs/contributors"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "contributte/application": "^0.5.1",
    "contributte/bootstrap": "^0.6.0",
    "contributte/cache": "^0.6.0",
    "contributte/console": "^0.10.1",
    "contributte/di": "^0.5.5",
    "contributte/forms": "^0.5.1",
    "contributte/http": "^0.4.0",
    "contributte/latte": "^0.5.0",
    "contributte/nextras-orm-query-object": "0.5.0",
    "contributte/security": "^0.4.0",
    "contributte/tracy": "^0.5.0",
    "contributte/utils": "^0.5.2",
    "nette/robot-loader": "^3.2.0",
    "nextras/dbal": "4.0.4",
    "nextras/migrations": "3.2.0",
    "nextras/orm": "4.0.6",
    "wavevision/nette-webpack": "^2.0.0",
    "wavevision/props-control": "^6.2.1",
    "wavevision/utils": "^2.12"
  },
  "require-dev": {
    "contributte/dev": "^0.3.0",
    "nextras/orm-phpstan": "^1.0.0",
    "ninjify/nunjuck": "^0.4",
    "ninjify/qa": "^0.14",
    "phpstan/phpstan": "^1.4.0",
    "phpstan/phpstan-nette": "^1.0.0",
    "wavevision/di-service-annotation": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "App\\Model\\": "app/model",
      "App\\Modules\\": "app/modules"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/cases"
    ]
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
